server {
    listen 80;
    server_name docsign.smartdjango.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /var/sites/docsign;
    }

    location / {
        include         uwsgi_params;
        uwsgi_pass      unix:/run/uwsgi/docsign.sock;
    }
}
